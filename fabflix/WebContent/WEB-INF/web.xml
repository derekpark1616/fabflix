<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>FabflixMovie</display-name>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>fabflix.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/store.Login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>MovieListView</servlet-name>
		<jsp-file>/WEB_INF/MovieListView.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>MovieListView</servlet-name>
		<url-pattern>/movie-list</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SingleMovieModule</servlet-name>
		<servlet-class>fabflix.SingleMovieServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SingleMovieModule</servlet-name>
		<url-pattern>/movie.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SingleMovieAjax</servlet-name>
		<servlet-class>fabflix.AjaxPopUpMovieInfoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SingleMovieAjax</servlet-name>
		<url-pattern>/movie.pop</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SingleMovieAjaxDBCP</servlet-name>
		<servlet-class>fabflix.DBCPAjaxPopUpMovieInfoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SingleMovieAjaxDBCP</servlet-name>
		<url-pattern>/movie.popDBCP</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>MoviesListModule</servlet-name>
		<servlet-class>fabflix.SrchStarServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MoviesListModule</servlet-name>
		<url-pattern>/star.goo</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>MoviesListModuleDBCP</servlet-name>
		<servlet-class>fabflix.DBCPSrchStarServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MoviesListModuleDBCP</servlet-name>
		<url-pattern>/star.gooDBCP</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SingleStarModule</servlet-name>
		<servlet-class>fabflix.StarListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SingleStarModule</servlet-name>
		<url-pattern>/star.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SingleStarModuleDBCP</servlet-name>
		<servlet-class>fabflix.DBCPStarListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SingleStarModuleDBCP</servlet-name>
		<url-pattern>/star.doDBCP</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SingleGenreModule</servlet-name>
		<servlet-class>fabflix.GenreListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SingleGenreModule</servlet-name>
		<url-pattern>/genre.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SingleGenreModuleDBCP</servlet-name>
		<servlet-class>fabflix.DBCPGenreListServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SingleGenreModuleDBCP</servlet-name>
		<url-pattern>/genre.doDBCP</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Movie</servlet-name>
		<servlet-class>fabflix.SrchMovieServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Movie</servlet-name>
		<url-pattern>/movie.go</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SingleMovieModuleDBCP</servlet-name>
		<servlet-class>fabflix.DBCPSingleMovieServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SingleMovieModuleDBCP</servlet-name>
		<url-pattern>/movie.DBCPDo</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>MovieTitle</servlet-name>
		<servlet-class>fabflix.Bg2Servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MovieTitle</servlet-name>
		<url-pattern>/movie.ok</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>MovieGenre</servlet-name>
		<servlet-class>fabflix.Bt2Servlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MovieGenre</servlet-name>
		<url-pattern>/movie.o</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ShoppingCart</servlet-name>
		<servlet-class>fabflix.shorServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ShoppingCart</servlet-name>
		<url-pattern>/movie.shop</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>Shopping</servlet-name>
		<servlet-class>fabflix.purchaseServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Shopping</servlet-name>
		<url-pattern>/movie.shp</url-pattern>
	</servlet-mapping>
	<session-config>
		<session-timeout>30</session-timeout>
	</session-config>
	<listener>
		<listener-class>fabflix.DBCPoolingListener</listener-class>
	</listener>
	<resource-ref>
		<description>DB Connection Pooling</description>
		<res-ref-name>jdbc/moviedb</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>
	<resource-ref>
		<description>DB Connection Pooling</description>
		<res-ref-name>jdbc/moviedb_emp</res-ref-name>
		<res-type>javax.sql.DataSource</res-type>
		<res-auth>Container</res-auth>
	</resource-ref>
	<servlet>
		<servlet-name>PoolingMovieSearch</servlet-name>
		<servlet-class>fabflix.DBCPSrchMovieServlet</servlet-class>
		<load-on-startup>0</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>PoolingMovieSearch</servlet-name>
		<url-pattern>/movie.doDBCP</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>PoolingEmployeeAction1</servlet-name>
		<servlet-class>fabflix.DBCPStoredProcServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>PoolingEmployeeAction1</servlet-name>
		<url-pattern>/movie.empDBCP</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>PoolingEmployeeAction2</servlet-name>
		<servlet-class>fabflix.DBCPInsertStarServlet</servlet-class>
		<load-on-startup>2</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>PoolingEmployeeAction2</servlet-name>
		<url-pattern>/star.empDBCP</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>login.html</welcome-file>
		<welcome-file>../_dashboard/empaccess.html</welcome-file>
	</welcome-file-list>
</web-app>